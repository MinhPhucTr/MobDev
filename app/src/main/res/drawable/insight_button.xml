<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <size
                android:width="48dp"
                android:height="48dp" />
            <corners android:radius="@dimen/dp20" />
            <stroke
                android:width="2px"
                android:color="@color/primary"
                android:dashWidth="@dimen/dp4"
                android:dashGap="@dimen/dp4" />
        </shape>
    </item>

    <item
        android:width="@dimen/dp32"
        android:height="@dimen/dp32"
        android:drawable="@drawable/add_insight_outlined"
        android:gravity="center"
        >
    </item>

</layer-list>